@model EntityLayer.Concrete.Contact

@{
    ViewData["Title"] = "ContactDetails";
    Layout = "~/Areas/Admin/Views/Shared/AdminLayout.cshtml";
}

<h1>ContactDetails</h1>

<div>
    <h4>Contact</h4>
    <hr />
    <dl class="row">
       
        <dt class = "col-sm-2">
            @Html.DisplayName("İsim")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ContactUserName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayName("Mail")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ContactMail)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayName("Konu")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ContactSubject)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ContactMessage)
        </dt>
        <dd class="col-sm-10">
            @Html.TextAreaFor(model=>model.ContactMessage,10,3, new { @readonly = "readonly " })
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayName("Tarih")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ContactDate)
        </dd>
        
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { /* id = Model.PrimaryKey */ }) |
    <a asp-action="Index">Back to List</a>
</div>
