@model IEnumerable<CategoryWithBlogCount>
<div class="tech-btm">
    <h4>Kategoriler</h4>
    <ul class="list-group single">
        @foreach (var item in Model)
        {
            <li class="list-group-item d-flex justify-content-between align-items-center">
                @item.CategoryName
                <span class="badge badge-primary badge-pill">@item.BlogCount</span>
            </li>
        }


    </ul>
</div>